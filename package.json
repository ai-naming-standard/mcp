    "ai-naming-standard",
    "naming-convention",
    "file-naming",
    "claude",
    "claude-desktop",
    "cursor",
    "chatgpt",
    "windsurf",
    "ai-development",
    "coding-standards",
    "automation",
    "microservices",
    "project-structure",
    "multi-language",
    "i18n",
    "chinese",
    "korean",
    "japanese",
    "multilingual",
    "self-documenting",
    "knowledge-persistence",
    "ai-readable",
    "governance",
    "audit-trail",
    "natural-language",
    "ainaming-cli"
  ],
  "homepage": "https://ai-naming-standard.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ai-naming-standard/v6.git"
  },
  "bugs": {
    "url": "https://github.com/ai-naming-standard/v6/issues"
  },
  "author": {
    "name": "AI Naming Standard Organization",
    "url": "https://github.com/ai-naming-standard"
  },
  "contributors": [
    {
      "name": "Hyun Seok Yang",
      "role": "v5.0.2 Foundation"
    },
    {
      "name": "AI Naming Standard Contributors",
      "role": "v6.0.0 Evolution"
    }
  ],
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "ai-naming-standard-mcp": "./bin/start.js",
    "ainaming": "./bin/ainaming.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "node test/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "mcp": {
    "type": "server",
    "protocol": "stdio",
    "capabilities": [
      "tools",
      "resources"
    ],
    "version": "6.0.0",
    "features": [
      "multi-ai-orchestration",
      "naming-wizard",
      "dependency-graph",
      "governance-layer",
      "role-differentiation",
      "natural-language-conversion",
      "audit-trail",
      "meta-templates"
    ]
  },
  "ainaming": {
    "cli_version": "6.0.0",
    "commands": [
      "init",
      "add",
      "deps",
      "sync",
      "validate",
      "export",
      "upgrade",
      "scan"
    ]
  }
}
